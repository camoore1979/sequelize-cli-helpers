{
  "name": "sequelize-cli-helpers",
  "version": "0.1.0",
  "description": "Better Sequelize CLI tools.",
  "main": "index.js",
  "bin": {
    "sequelize-cli-helpers": "./index.js"
  },
  "scripts": {
    "debug": "node --nolazy --inspect-brk=9229 index.js touch",
    "lint": "eslint \"src/**/*.js\"",
    "fixlint": "eslint --fix \"src/**/*.js\"",
    "prepush": "npm run lint && npm run test",
    "start": "node index",
    "test": "babel-tape-runner \"./src/**/*.test.js\" \"./test/**/*.test.js\" | node_modules/.bin/tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camoore1979/sequelize-cli-helpers.git"
  },
  "keywords": [
    "sequelize",
    "sequelize-cli",
    "sequelize helpers"
  ],
  "author": "Chad Allen Moore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/camoore1979/sequelize-cli-helpers/issues"
  },
  "homepage": "https://github.com/camoore1979/sequelize-cli-helpers#readme",
  "contributors": [
    "Chad Allen Moore <mr.chad.moore@gmail.com> (http://www.chadamoore.com/)"
  ],
  "engines": {
    "node": "8.4.0"
  },
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "chalk": "^2.4.2",
    "filenamify": "^2.1.0",
    "find-up": "^3.0.0",
    "get-stream": "^5.0.0",
    "handlebars": "^4.1.0",
    "inquirer": "^6.2.2",
    "moment": "^2.24.0",
    "prettier-eslint": "^8.8.2",
    "rxjs": "^6.4.0",
    "string-to-stream": "^1.1.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "babel-tape-runner": "^3.0.0",
    "dumper.js": "^1.3.0",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "husky": "^1.3.1",
    "prettier-eslint-cli": "^4.7.1",
    "proxyquire": "^2.1.0",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush",
      "post-commit": "prettier-eslint --write"
    }
  }
}
